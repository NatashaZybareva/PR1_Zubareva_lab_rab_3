    $(document).ready(function () { 
        $("button").click(function () { 
            $("#div5").animate({left: '100px' });
            $("#div9").animate({left: '100px' }); 
        }); 
        $("#div2").click(function () {
		$('#div2').delay(50).animate({marginTop:+30},0);
	});
        $("#div4").click(function () {
		$('#div4').delay(50).animate({marginTop:+30},0);
	});
        $("#div6").click(function () {
		$('#div6').delay(50).animate({marginTop:+30},0);
	});
        $("#div8").click(function () {
		$('#div8').delay(50).animate({marginTop:+30},0);
	});
        $("#div10").click(function () {
		$('#div10').delay(50).animate({marginTop:+30},0);
	});
    }); 
</script>  
